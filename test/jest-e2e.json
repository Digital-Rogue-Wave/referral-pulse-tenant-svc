{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@mod/(.*)$": "<rootDir>/../src/$1",
    "^@domains/(.*)$": "<rootDir>/../src/domains/$1"
  }
}

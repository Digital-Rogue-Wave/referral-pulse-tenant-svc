{
  "name": "referral-pulse-billing-refactored-svc",
  "version": "1.0.0",
  "description": "Production-ready NestJS microservice for billing management",
  "author": "ReferralAI",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "pnpm typeorm -- migration:generate -d src/database/data-source.ts",
    "migration:run": "pnpm typeorm -- migration:run -d src/database/data-source.ts",
    "migration:revert": "pnpm typeorm -- migration:revert -d src/database/data-source.ts",
    "pnpm:dedupe": "pnpm dedupe",
    "pnpm:update": "pnpm update --latest",
    "clean": "rimraf dist node_modules coverage",
    "clean:install": "pnpm clean && pnpm install"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/client-s3": "^3.550.0",
    "@aws-sdk/client-sns": "^3.550.0",
    "@aws-sdk/client-sqs": "^3.550.0",
    "@aws-sdk/credential-provider-node": "^3.962.0",
    "@aws-sdk/lib-storage": "^3.550.0",
    "@aws-sdk/s3-request-presigner": "^3.550.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/microservices": "^10.3.8",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.2.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/instrumentation-aws-sdk": "^0.64.1",
    "@opentelemetry/instrumentation-express": "^0.57.1",
    "@opentelemetry/instrumentation-http": "^0.208.0",
    "@opentelemetry/instrumentation-ioredis": "^0.57.0",
    "@opentelemetry/instrumentation-pg": "^0.61.2",
    "@opentelemetry/propagator-b3": "^2.2.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "@smithy/protocol-http": "^5.3.7",
    "@smithy/signature-v4": "^5.3.7",
    "@ssut/nestjs-sqs": "^3.0.0",
    "axios": "^1.6.8",
    "bullmq": "^5.66.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "eventemitter2": "^6.4.9",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "jwks-rsa": "^3.2.0",
    "lru-cache": "^10.2.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "nestjs-cls": "^4.3.0",
    "nestjs-paginate": "^12.8.3",
    "nestjs-pino": "^4.0.0",
    "opossum": "^8.1.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "pino": "^8.20.0",
    "pino-http": "^9.0.0",
    "pino-loki": "^3.0.0",
    "pino-pretty": "^11.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "simdjson": "^0.9.2",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0",
    "ulid": "^2.3.0",
    "zod": "^3.22.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.16",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.12",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^20.12.7",
    "@types/opossum": "^8.1.6",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "node-gyp": "^12.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.1.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/$1",
      "^@domains/(.*)$": "<rootDir>/domains/$1",
      "^@common/(.*)$": "<rootDir>/common/$1",
      "^@config/(.*)$": "<rootDir>/config/$1"
    }
  }
}
